import { createApp } from "/node_modules/.vite/deps/vue.js?v=c20d0c91"
import { createVuetify } from "/node_modules/.vite/deps/vuetify.js?v=c20d0c91"
import "/node_modules/@mdi/font/css/materialdesignicons.css"
import { aliases, mdi } from "/node_modules/.vite/deps/vuetify_iconsets_mdi.js?v=c20d0c91"

import App from "/src/App.vue"
import router from "/src/router/index.js?t=1700617726942"
import "/node_modules/vuetify/lib/styles/main.css"
import * as components from "/node_modules/.vite/deps/vuetify_components.js?v=c20d0c91"
import * as directives from "/node_modules/.vite/deps/vuetify_directives.js?v=c20d0c91"
import { createPinia } from "/node_modules/.vite/deps/pinia.js?v=c20d0c91"
import piniaPluginPersistedState from "/node_modules/.vite/deps/pinia-plugin-persistedstate.js?v=c20d0c91"
const vuetify = createVuetify({
  components,
  directives,

  icons: {
    defaultSet: 'mdi',
    aliases,
    sets: {
      mdi
    }
  }
})

const app = createApp(App)

// pinia-plugin-persistedstate
import { instance } from "/src/api/axios.js"

app.provide('axios', instance)
app.use(createPinia().use(piniaPluginPersistedState))
app.use(router)
app.use(vuetify)
app.use(aliases)
app.use(mdi)

app.mount('#app')
